psql -U postgres
CREATE TABLE Artists (
	id varchar(50) PRIMARY KEY,
	name varchar(50),
	hotttnesss decimal
);

COPY Artists FROM '/root/w205_final_project/Artists.txt' DELIMITER ',' CSV;

CREATE TABLE Songs (
	artist_id varchar(50),
	artist_name varchar(50),
	artist_hotttnesss decimal,
	id varchar(50) PRIMARY KEY,
	title varchar(50)
);

COPY Songs FROM '/root/w205_final_project/Songs.txt' DELIMITER ',' CSV;